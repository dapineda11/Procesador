		last_EstadosQ_exp[51] = EstadosQ_expected_prev[51];
	end
	if (
		( EstadosQ_expected_prev[52] !== 1'bx ) && ( EstadosQ_prev[52] !== EstadosQ_expected_prev[52] )
		&& ((EstadosQ_expected_prev[52] !== last_EstadosQ_exp[52]) ||
			on_first_change[4])
	)
	begin
		$display ("ERROR! Vector Mismatch for output port EstadosQ[52] :: @time = %t",  $realtime);
		$display ("     Expected value = %b", EstadosQ_expected_prev);
		$display ("     Real value = %b", EstadosQ_prev);
		nummismatches = nummismatches + 1;
		on_first_change[4] = 1'b0;
		last_EstadosQ_exp[52] = EstadosQ_expected_prev[52];
	end
	if (
		( EstadosQ_expected_prev[53] !== 1'bx ) && ( EstadosQ_prev[53] !== EstadosQ_expected_prev[53] )
		&& ((EstadosQ_expected_prev[53] !== last_EstadosQ_exp[53]) ||
			on_first_change[4])
	)
	begin
		$display ("ERROR! Vector Mismatch for output port EstadosQ[53] :: @time = %t",  $realtime);
		$display ("     Expected value = %b", EstadosQ_expected_prev);
		$display ("     Real value = %b", EstadosQ_prev);
		nummismatches = nummismatches + 1;
		on_first_change[4] = 1'b0;
		last_EstadosQ_exp[53] = EstadosQ_expected_prev[53];
	end

	trigger <= ~trigger;
end
initial 

begin 
$timeformat(-12,3," ps",6);
#1000000;
if (nummismatches > 0)
	$display ("%d mismatched vectors : Simulation failed !",nummismatches);
else
	$display ("Simulation passed !");
$finish;
end 
endmodule

